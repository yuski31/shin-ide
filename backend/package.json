{
  "name": "@web-ide/backend",
  "version": "1.0.0",
  "description": "Backend server for the web-based IDE",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx src/database/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.5",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "@prisma/client": "^5.8.1",
    "redis": "^4.6.12",
    "ioredis": "^5.3.2",
    "node-pty": "^1.0.0",
    "chokidar": "^3.5.3",
    "ws": "^8.16.0",
    "uuid": "^9.0.1",
    "mime-types": "^2.1.35",
    "archiver": "^6.0.1",
    "unzipper": "^0.10.14",
    "tar": "^6.2.0",
    "graceful-fs": "^4.2.11",
    "node-fetch": "^3.3.2",
    "axios": "^1.6.5",
    "sharp": "^0.33.2",
    "nodemailer": "^6.9.8",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "openai": "^4.28.0",
    "@anthropic-ai/sdk": "^0.17.1",
    "@google/generative-ai": "^0.2.1",
    "@ai-sdk/openai": "^0.0.9",
    "@ai-sdk/anthropic": "^0.0.9",
    "@ai-sdk/google": "^0.0.9",
    "@ai-sdk/amazon-bedrock": "^0.0.3",
    "@ai-sdk/cohere": "^0.0.3",
    "@ai-sdk/mistral": "^0.0.9",
    "ai": "^3.0.12",
    "@webcontainer/api": "^1.1.9",
    "jspdf": "^2.5.1",
    "jszip": "^3.10.1",
    "isomorphic-git": "^1.25.6",
    "@remix-run/node": "^2.5.1",
    "@remix-run/express": "^2.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.5",
    "@types/uuid": "^9.0.7",
    "@types/mime-types": "^2.1.4",
    "@types/archiver": "^6.0.2",
    "@types/tar": "^6.1.10",
    "@types/graceful-fs": "^4.1.9",
    "@types/nodemailer": "^6.4.14",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "typescript": "^5.7.2",
    "tsx": "^4.7.0",
    "vitest": "^1.2.0",
    "supertest": "^6.3.4",
    "@types/supertest": "^6.0.2",
    "prisma": "^5.8.1"
  }
}
